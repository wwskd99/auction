<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- Java interface와 namespace를 맞추어준다. -->
<mapper namespace="org.zerock.mapper.GPSMapper">


<insert id="registProductGPS">
insert into product_gps (product_id, latitude, longitude) values (#{product_id},#{latitude},#{longitude});
</insert>

<delete id="deleteGPS">
  delete from product_gps where product_id = #{product_id};
</delete>



</mapper>
